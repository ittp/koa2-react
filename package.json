    "mongo-db": "^1.0.2",
    "npm": "^5.0.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-native": "^0.45.1",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-tap-event-plugin": "^2.0.1",
    "require-hacker": "^3.0.0"
  },
