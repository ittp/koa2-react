{
  "name": "koa2-react",
  "version": "1.0.5",
  "description": "A porject with koa2,react,mobx on server side render",
  "repository": "https://github.com/windwithfo/koa2-react.git",
  "main": "server/server.js",
  "scripts": {
    "start": "forever start -c 'babel-node' --colors -n -a -l forever.log --minUptime 1000 --spinSleepTime 10000 build/server.js",
    "stop": "forever stop build/server.js",
    "dev": "nodemon --config config/nodemon.json server/server.js",
    "build": "babel-node configs/build.js --display-error-details && zip -r react.zip build",
    "buildall": "babel-node configs/build.js --display-error-details && zip -r react.zip build node_modules",
    "lint": "eslint --ext .js server",
    "create": "babel-node scripts/create.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "reat",
    "koa2",
    "mobx",
    "ssr"
  ],
  "author": "dongkunshan(windwihtfo@yeah.net)",
  "license": "ISC",
  "dependencies": {
    "koa": "^2.5.0",
    "koa-bodyparser": "^4.2.0",
    "koa-jsonp": "^2.0.2",
    "koa-proxies": "^0.6.2",
    "koa-router": "^7.4.0",
    "koa-static": "^4.0.2",
    "koa-views": "^6.1.3",
    "mobx": "^3.5.1",
    "mobx-react": "^4.4.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-loadable": "^5.3.1",
    "react-router-dom": "^4.2.2",
    "webpack-isomorphic-tools": "^3.0.5"
  },
  "devDependencies": {
    "@types/node": "^9.4.6",
    "@types/react": "^16.0.38",
    "@types/react-dom": "^16.0.4",
    "@types/react-router-dom": "^4.2.4",
    "autoprefixer": "^8.0.0",
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "css-loader": "^0.28.10",
    "eventsource-polyfill": "^0.9.6",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.9",
    "html-webpack-plugin": "^2.30.1",
    "koa-webpack": "^1.0.0",
    "node-sass": "^4.7.2",
    "nodemon": "^1.15.1",
    "postcss-cssnext": "^3.1.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.2",
    "stylelint": "^9.1.1",
    "stylelint-processor-html": "^1.0.0",
    "stylelint-webpack-plugin": "^0.10.3",
    "ts-loader": "^3.5.0",
    "typescript": "^2.7.2",
    "uglifyjs-webpack-plugin": "^1.2.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.5.5",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-merge": "^4.1.2"
  }
}
