{
  "name": "koa2-react",
  "version": "1.0.5",
  "description": "A porject with koa2,react,mobx on server side render",
  "repository": "https://github.com/windwithfo/koa2-react.git",
  "main": "server/server.js",
  "scripts": {
    "start": "forever start -c 'babel-node' --colors -n -a -l forever.log --minUptime 1000 --spinSleepTime 10000 build/server.js",
    "stop": "forever stop build/server.js",
    "release": "npm run build && npm run start",
    "dev": "npm run fe & npm run rd",
    "rd": "nodemon --config configs/nodemon.json server/server.js",
    "fe": "babel-node node_modules/.bin/webpack-dev-server --config configs/webpack.config.js --devtool eval --progress --hotOnly",
    "build": "babel-node configs/build.js --display-error-details && zip -r react.zip build",
    "build-rd": "babel-node configs/build-rd.js --display-error-details",
    "buildall": "babel-node configs/build.js --display-error-details && zip -r react.zip build node_modules",
    "lint": "eslint --ext .js server",
    "create": "babel-node scripts/create.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "reat",
    "koa2",
    "mobx",
    "ssr"
  ],
  "author": "dongkunshan(windwihtfo@yeah.net)",
  "license": "ISC",
  "dependencies": {
    "antd": "^2.11.0",
    "antd-mobile": "^1.4.0",
    "install": "^0.10.1",
    "ioredis": "^3.1.1",
